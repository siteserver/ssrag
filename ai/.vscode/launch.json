{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["main:app", "--reload", "--port", "6603"],
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "Python: Celery",
      "type": "python",
      "request": "launch",
      "module": "celery",
      "args": ["-A", "celery_app", "worker", "--loglevel=info", "--pool=solo"],
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "env": {
        "HF_ENDPOINT": "https://hf-mirror.com"
      }
    }
  ]
}
