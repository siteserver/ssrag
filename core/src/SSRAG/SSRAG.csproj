<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <PackageId>SSRAG</PackageId>
    <Version>0.9.19</Version>
    <Authors>SSRAG</Authors>
    <Company>SSRAG</Company>
    <PackageDescription>SSRAG .NET Core abstractions for developers.</PackageDescription>
    <Copyright>Copyright Â© SSRAG</Copyright>
    <RepositoryUrl>https://github.com/siteserver/ssrag/</RepositoryUrl>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageIcon>logo.png</PackageIcon>
    <PackageLicenseExpression>AGPL-3.0-or-later</PackageLicenseExpression>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <CompressionEnabled>false</CompressionEnabled>
  </PropertyGroup>

  <ItemGroup>
