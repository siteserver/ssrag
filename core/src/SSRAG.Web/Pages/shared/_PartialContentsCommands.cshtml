<div id="commands">

  <button type="button" class="el-button el-button--primary el-button--mini is-plain gradient-button"
    :disabled="!permissions.isAdd" v-on:click="btnAIWriterClick" style="display: none;">
    <span class="ant-btn-icon">
      <svg t="1738898735804" class="icon" viewBox="0 0 1024 1024" xmlns="http://www.w3.org/2000/svg" p-id="22142"
        width="1em" height="1em">
