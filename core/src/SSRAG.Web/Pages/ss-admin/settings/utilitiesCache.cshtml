@page
@{ Layout = "_Layout"; }

<el-table class="headerless" :data="keys" style="width: 100%">
  <el-table-column label="缓存键">
    <template slot-scope="scope">
      <strong>{{scope.row}}</strong>
    </template>
  </el-table-column>
</el-table>

<el-divider></el-divider>
<div style="height: 10px"></div>
<el-button size="small" type="primary" v-on:click="btnCleanClick">清空缓存</el-button>
<el-button size="small" type="primary" v-on:click="btnRestartClick">重启系统</el-button>

@section Scripts{
<script src="/sitefiles/assets/js/admin/settings/utilitiesCache.js" type="text/javascript"></script> }